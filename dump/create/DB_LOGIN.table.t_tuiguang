CREATE TABLE `t_tuiguang` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `tuiplayerid` varchar(32) DEFAULT NULL COMMENT '推广者',
  `beituiplayerid` varchar(32) DEFAULT NULL COMMENT '被推广者',
  `optime` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `tplayerid_index` (`tuiplayerid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8