# The server to client proto defines.
.package {
	type    0 : integer
	session 1 : integer
  ud      2 : integer
}

# The hello message.
hello 1 {
  request {
    text 0 : string
  }
}

# The role info structure.
.role {
  id      0 : string
  name    1 : string
  job     2 : integer
  level   3 : integer
}

# The role list.
roles 2 {
  request {
    list    0 : *role
  }
}

# Other login.
other_login 3 {
  request {}
}
